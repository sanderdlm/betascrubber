{
    "name": "sanderdlm/skeleton",
    "description": "Skeleton for a PHP project",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "twig/twig": "^3.7",
        "php-di/php-di": "^7.0",
        "nikic/fast-route": "^1.3",
        "middlewares/fast-route": "^2.0",
        "middlewares/error-handler": "^3.0",
        "middlewares/request-handler": "^2.0",
        "laminas/laminas-diactoros": "^3.3",
        "laminas/laminas-httphandlerrunner": "^2.9",
        "symfony/process": "^7.3",
        "aws/aws-sdk-php": "^3.359",
        "vlucas/phpdotenv": "^5.6",
        "monolog/monolog": "^3.9"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "symfony/var-dumper": "^7.0",
        "phpunit/phpunit": "^12.4"
    },
    "scripts": {
        "test": "phpunit --testdox",
        "test:unit": "phpunit --testdox --testsuite 'Unit Tests'",
        "test:integration": "phpunit --testdox --testsuite 'Integration Tests'",
        "test:coverage": "phpunit --coverage-html coverage"
    }
}
